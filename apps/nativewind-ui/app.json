{
  "expo": {
    "name": "react-native-ui",
    "slug": "react-native-ui",
    "version": "1.0.0",
    "scheme": "reactnativeui",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
